<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity OrderProduct.
    -->
    <changeSet id="20191030094616-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id_id"
                                 baseTableName="order_product"
                                 constraintName="fk_order_product_product_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="order_id_id"
                                 baseTableName="order_product"
                                 constraintName="fk_order_product_order_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="order_product"
                                 constraintName="fk_order_product_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="order_product"
                                 constraintName="fk_order_product_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

    </changeSet>
</databaseChangeLog>
